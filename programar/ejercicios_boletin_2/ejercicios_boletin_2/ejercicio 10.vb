Module ejercicio_10
    Sub main()

        Dim horas, dias, horastotales As Single
        Dim irpf, salarioneto, salariobruto, salariohora As Double

        'entrada
        Console.WriteLine("horas diarias: ")
        horas = Convert.ToInt32(Console.ReadLine())

        Console.WriteLine("Dias del mes trabajados: ")
        dias = Convert.ToInt32(Console.ReadLine())

        Console.WriteLine("salario por hora: ")
        salariohora = Convert.ToInt32(Console.ReadLine)

        'calculos
        Convert.ToSingle(horas * dias = horastotales)

        Convert.ToDouble(horastotales * salariohora = salariobruto)

        Console.WriteLine("salariobruto = " & salariobruto)
        Console.ReadLine()

        Console.WriteLine(" IRPF = " & irpf)




    End Sub
End Module
